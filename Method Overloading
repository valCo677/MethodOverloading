public class Main {
    static int plusMethodInt( int x, int y ) {
        return x * y;
    }

    static double plusMethodDouble( double x, double y ) {
        return x * y;
    }

    public static void main( String[] args ) {
        int myNum1 = plusMethodInt ( 8, 5 );
        double myNum2 = plusMethodDouble ( 5.5, 3.2 );
        System.out.println ( "int : " + myNum1 + " meter" );
        System.out.println ( "double : " + myNum2+ " meter" );

    }

}
